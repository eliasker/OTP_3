	<groupId>com.ryhma_3</groupId>
	<artifactId>kaiku</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>kaiku</name>
	<description>OTP project, ryhm√§ 3</description>

	<properties>
		<java.version>1.8</java.version>
	</properties>

	<dependencies>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-starter-data-rest</artifactId>
		</dependency>
		<dependency>
